+cleanup_old_scripts() {
+    log_section "Cleaning Up Old Scripts"
+    
+    log_info "Moving deprecated scripts..."
+    
+    # Create deprecated directory if not exists
+    mkdir -p $PROJECT_ROOT/deprecated_scripts/archived_$(date +%Y%m%d)
